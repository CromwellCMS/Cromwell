    "@types/react-dom": "18.0.8",
    "@types/testing-library__jest-dom": "5.14.1",
    "@types/testing-library__react": "10.2.0",
    "@types/throttle-debounce": "2.1.0",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "clsx": "1.1.1",
    "date-fns": "2.29.3",
    "eslint": "8.27.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-react": "7.31.10",
    "jest": "29.2.2",
    "react-is": "18.2.0",
    "react-resize-detector": "7.1.2",
    "react-toastify": "9.1.1",
    "sass": "1.38.0",
    "throttle-debounce": "3.0.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "@cromwell/cli": "^2.1.6",
    "@cromwell/cms": "^2.1.6",
    "@cromwell/core": "^2.0.13",
    "@cromwell/core-frontend": "^2.1.4",
    "@cromwell/plugin-main-menu": "^1.0.35",
    "@cromwell/plugin-newsletter": "^1.0.25",
    "@cromwell/plugin-paypal": "^1.0.3",
    "@cromwell/plugin-product-filter": "^1.0.35",
    "@cromwell/plugin-product-showcase": "^1.0.26",
    "@cromwell/plugin-stripe": "^1.0.13",
    "@cromwell/renderer": "^2.1.5",
    "@cromwell/toolkit-commerce": "^1.0.4",
    "@cromwell/utils": "^2.0.17",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
