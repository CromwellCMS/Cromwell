{
  "name": "@cromwell/theme-store",
  "version": "1.0.0",
  "author": "cromwell",
  "license": "ISC",
  "main": "package.json",
  "files": [
    "build",
    "static"
  ],
  "scripts": {
    "build": "npx --no-install cromwell b",
    "watch": "npx --no-install cromwell b -w"
  },
  "dependencies": {
    "@apollo/client": "3.2.1",
    "@cromwell/cli": "1.0.0",
    "@cromwell/core": "1.1.0",
