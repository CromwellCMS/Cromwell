    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-ts-compiler": "^1.0.9",
    "typescript": "^4.0.3"
  },
  "cromwell": {
    "type": "theme",
    "title": "Store",
    "previewImage": "static/shopping-cart.png",
    "description": "Online store theme",
    "plugins": [
      "@cromwell/plugin-customer-reviews",
      "@cromwell/plugin-product-filter",
      "@cromwell/plugin-main-menu",
      "@cromwell/plugin-product-showcase"
    ],
    "themes": [
      "@cromwell/theme-store"
    ],
    "frontendDependencies": [
      "mobx",
      "mobx-react",
      "react-swipeable-views",
      "@material-ui/core",
      {
        "name": "@material-ui/lab",
        "externals": [
          {
            "usedName": "@material-ui/core/styles",
