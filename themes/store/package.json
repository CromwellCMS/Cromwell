{
  "name": "@cromwell/theme-store",
  "version": "2.0.10",
  "license": "MIT",
  "repository": "https://github.com/CromwellCMS/Cromwell",
  "author": "Astrex LLC",
  "bugs": {
    "url": "https://github.com/CromwellCMS/Cromwell/issues"
  },
  "files": [
    "build",
    "static",
    "cromwell.config.js"
  ],
  "scripts": {
    "build": "npx --no-install cromwell build",
    "test": "cd tests && npx --no-install jest",
    "watch": "npx --no-install cromwell b -w",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --ignore-unknown \"**/*.+(js|jsx|json|css|md|ts|tsx)\""
  },
  "cromwell": {
    "type": "theme",
    "title": "Default Online Store",
    "image": "static/theme-store.png",
    "excerpt": "Default CromwellCMS Online Store theme",
    "description": "<p>Full-featured online store theme.</p><p><br></p><ul><li>Customer account page with order history</li><li>Home page with gallery and banners</li><li>Product categories with filtration</li><li>Search</li><li>Customer reviews</li><li>Quick-view</li><li>Save product as favorite</li><li>Related products</li><li>Blog</li></ul>",
    "author": "Cromwell CMS",
    "plugins": [
      "@cromwell/plugin-product-filter",
      "@cromwell/plugin-main-menu",
      "@cromwell/plugin-product-showcase",
      "@cromwell/plugin-newsletter",
      "@cromwell/plugin-stripe",
      "@cromwell/plugin-paypal"
    ],
    "frontendDependencies": [
      "mobx",
      "mobx-react",
      "react-resize-detector",
      "@mui/material",
      "@mui/styles",
      "@mui/lab",
      "@mui/system",
      "date-fns",
      "throttle-debounce",
      "tslib"
    ],
    "firstLoadedDependencies": [
      "mobx",
      "mobx-react",
      "date-fns",
      "react-resize-detector",
      "@mui/material",
      "@mui/styles",
      "@mui/system",
      "react-toastify",
      "prop-types",
      "hoist-non-react-statics",
      "react-transition-group",
      "clsx",
      "react-is",
      "object-assign",
      "jss",
      "jss-plugin-rule-value-function",
      "jss-plugin-global",
      "jss-plugin-nested",
      "jss-plugin-camel-case",
      "jss-plugin-default-unit",
      "jss-plugin-vendor-prefixer",
      "jss-plugin-props-sort",
      "is-in-browser",
      "tiny-warning",
      "hyphenate-style-name",
      "css-vendor",
      "scheduler",
      "@wry/equality",
      "tslib",
      "graphql-tag",
      "symbol-observable",
      "fast-json-stable-stringify",
      "zen-observable",
      "ts-invariant",
      "optimism",
      "throttle-debounce",
      "pure-react-carousel",
      "@apollo/client"
    ]
  },
  "dependencies": {
    "@emotion/cache": "11.10.5",
    "@emotion/server": "11.10.0",
    "@mui/icons-material": "5.10.16",
    "@mui/lab": "5.0.0-alpha.111",
    "@mui/material": "5.10.17",
    "@mui/styles": "5.10.16",
    "mobx": "6.3.2",
    "mobx-react": "7.2.0",
    "reset-css": "5.0.1"
  },
  "devDependencies": {
    "@apollo/client": "3.7.1",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.12",
