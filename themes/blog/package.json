      "swiper",
      "react-toastify",
      "prop-types",
      "hoist-non-react-statics",
      "react-transition-group",
      "clsx",
      "react-is",
      "popper.js",
      "object-assign",
      "jss",
      "jss-plugin-rule-value-function",
      "jss-plugin-global",
      "jss-plugin-nested",
      "jss-plugin-camel-case",
      "jss-plugin-default-unit",
      "jss-plugin-vendor-prefixer",
      "jss-plugin-props-sort",
      "is-in-browser",
      "tiny-warning",
      "hyphenate-style-name",
      "css-vendor",
      "scheduler",
      "@wry/equality",
