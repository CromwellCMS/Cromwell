    "@types/react-dom": "16.9.8",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/throttle-debounce": "^2.1.0",
    "autoprefixer": "^9.8.6",
    "rollup": "^2.28.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-ts-compiler": "^1.0.12",
    "typescript": "^4.0.3"
  },
  "cromwell": {
    "type": "theme",
    "title": "Blog",
    "previewImage": "static/blog.png",
    "description": "Simple blog website",
    "plugins": [
      "@cromwell/plugin-product-filter",
      "@cromwell/plugin-main-menu",
      "@cromwell/plugin-product-showcase"
    ],
    "themes": [
      "@cromwell/theme-blog"
    ],
    "frontendDependencies": [
      "react-swipeable-views"
    ],
    "bundledDependencies": [
      "date-fns",
      "react-swipeable-views",
      "@material-ui/core",
      "@material-ui/styles",
      "@material-ui/utils",
