    "clsx": "1.1.1",
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-swipeable-views": "0.13.9",
    "react-toastify": "6.0.8",
    "reset-css": "5.0.1",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^13.0.2",
