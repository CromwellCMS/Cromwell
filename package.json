{
    "name": "@cromwell/root",
    "version": "1.0.0",
    "main": "startup.js",
    "private": true,
    "scripts": {
        "startup": "node ./startup.js",
        "build": "node ./startup.js build",
        "build:core": "npm run build --prefix ./system/core/common && npm run build --prefix ./system/core/backend && npm run build --prefix ./system/core/frontend",
        "dev": "node ./system/manager/startup.js development",
        "dev:win": "node ./system/manager/src/utils/windowsDev.js",
        "link:cli": "cd ./system/cli && npm link --only=production && yarn",
        "bundle:modules": "npx crw bm",
        "clean": "node system/cli/src/utils/cleanup.js",
        "win-remove-node-modules": "rmdir \"./node_modules\" /q /s",
        "test": "npm run test:server && npm run test:backend && npm run test:frontend && npm run test:admin && npm run test:cli && npm run test:utils",
        "test:server": "cd system/server && npm run test",
        "test:admin": "cd system/admin-panel && npm run test",
        "test:frontend": "cd system/core/frontend && npm run test",
        "test:backend": "cd system/core/backend && npm run test",
        "test:cli": "cd system/cli && npm run test",
        "test:utils": "cd system/utils && npm run test"
    },
    "workspaces": [
        "system/*",
        "system/core/*",
        "plugins/*",
        "themes/*"
    ],
    "resolutions": {
        "@apollo/client": "3.2.1",
        "@loadable/component": "5.14.1",
        "@material-ui/core": "4.11.2",
        "@material-ui/styles": "4.11.4",
        "@material-ui/utils": "4.11.2",
        "@material-ui/icons": "4.11.2",
        "@material-ui/lab": "4.0.0-alpha.57",
        "@material-ui/pickers": "3.3.10",
        "clsx": "1.1.1",
        "date-fns": "2.19.0",
        "graphql": "15.3.0",
        "graphql-tag": "2.12.4",
        "mobx": "5.15.7",
        "mobx-react": "6.3.1",
        "next": "10.1.3",
        "query-string": "6.13.4",
        "quill": "1.3.7",
        "pure-react-carousel": "1.27.6",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-html-parser": "2.0.2",
        "react-is": "17.0.1",
        "react-resize-detector": "6.7.1",
        "react-swipeable-views": "0.13.9",
        "react-toastify": "6.0.8",
        "react-image-lightbox": "5.1.1",
        "throttle-debounce": "3.0.1",
        "tslib": "2.1.0"
    },
    "cache-version": "7; modify this string to reset circleci cache"
}